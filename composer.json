        "ext-json": "*",
        "ext-session": "*",
        "ext-filter": "*",
        "nette/application": "^3.0",
        "nette/http": "^3.0",
        "tracy/tracy": "^2.6",
        "league/fractal": "^0.17.0",
        "tomaj/nette-bootstrap-form": "^2.0"
